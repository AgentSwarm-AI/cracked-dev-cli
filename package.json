{
  "name": "cracked-dev-cli",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "calc": "./bin/run.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "oclif": {
    "bin": "calc",
    "dirname": "calc",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "devDependencies": {
    "@oclif/test": "^4.1.0",
    "@types/jest": "^29.5.11",
    "eslint": "^9.15.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@oclif/core": "^4.0.32",
    "@oclif/plugin-help": "^6.2.16",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.2.2",
    "tsup": "^8.3.5",
    "tsyringe": "^4.8.0",
    "zod": "^3.23.8"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "wrap-ansi": "7.0.0"
  }
}
